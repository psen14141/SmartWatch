        <script>
            // common delete confirmation
            function DeleteConfirmation(serverUrl, formData = null, callBackFun = null, needCancelMsg = false, hedding = '削除しますか？', bodyContent = 'このデータを完全に消去しまか?') {
                swal({
                    title: hedding,
                    text: bodyContent,
                    type: 'warning',
                    buttons: {

                        confirm: {
                            text: '削除',
                            className: 'btn btn-danger'
                        },
                        cancel: {
                            visible: true,
                            text: 'キャンセル',
                            className: 'btn btn-primary'
                        }
                    }
                }).then((willDelete) => {
                    if (willDelete) {
                        $.ajax({
                            method: "POST",
                            url: serverUrl,
                            data: formData
                        })
                            .done(function (resposeData) {
                                if (callBackFun == null) {
                                    swal("正常に削除されました", {
                                        icon: "success",
                                        timer: 1500,
                                        buttons: {
                                            confirm: {
                                                className: 'btn btn-primary'
                                            }
                                        }
                                    });
                                    location.reload();
                                }
                                else {
                                    callBackFun(resposeData);
                                }
                            })
                            .fail(function () {
                                swal("削除できません", {
                                    buttons: {
                                        confirm: {
                                            className: 'btn btn-primary'
                                        }
                                    }
                                });
                            })
                            .error(function () {
                                swal("データ削除エラー", {
                                    buttons: {
                                        confirm: {
                                            className: 'btn btn-primary'
                                        }
                                    }
                                });
                            });
                    } else {
                        if (needCancelMsg) {
                            swal("操作をキャンセルします", {
                                timer: 1500,
                                buttons: {
                                    confirm: {
                                        className: 'btn btn-primary'
                                    }
                                }
                            });
                        }
                    }
                });
            }
        </script>
